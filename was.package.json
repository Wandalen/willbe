    "wfilesarchive" : "",
    "wfilesencoders" : "",
    "wstatestorage" : "",
    "wstatesession" : "",
    "wstager" : "",
    "wgraphextra" : "",
    "wselector" : "",
    "wresolver" : "",
    "wresolverextra" : "",
    "wcommandsaggregator" : "",
    "wcommandsconfig" : "",
    "wnamemapper" : "",
    "wtranspile" : "",
    "wnpmtools" : "",
    "wyamltools" : "",
    "wcensorbasic" : "",
    "tar" : "6.0.2",
    "gitconfiglocal" : "2.1.0",
    "open" : "7.0.4",
    "getos" : "3.2.1"
  },
  "devDependencies" : { "wTesting" : "", "eslint" : "7.1.0" },
